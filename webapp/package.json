  "scripts": {
    "analyze": "source-map-explorer ../build-webapp/static/js/**/*.js",
    "analyze-win": "source-map-explorer ../build-webapp/static/js/**/*.js --html source-explorer.html && explorer.exe source-explorer.html",
    "start": "CHOKIDAR_USEPOLLING=true PORT=22004 FAST_REFRESH=false react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "prettier-check": "prettier --no-error-on-unmatched-pattern --check '*.js' './tools/*.js' './src/**/*.js'",
    "prettier-fix": "prettier --no-error-on-unmatched-pattern --write '*.js' './tools/*.js' './src/**/*.js'",
    "eslint-check": "eslint --no-error-on-unmatched-pattern '*.js' './tools/*.js' './src/**/*.js'",
    "eslint-fix": "eslint --no-error-on-unmatched-pattern --fix '*.js' './tools/*.js' './src/**/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "prettier": "^2.8.3",
    "qrcode": "^1.5.1",
    "source-map-explorer": "^2.5.3"
  }
