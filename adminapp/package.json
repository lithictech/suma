    "@sentry/tracing": "^7.102.0",
    "axios": "^1.2.3",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "humps": "git+https://github.com/lithictech/humps.git#fb9844c8d37cea67ee9c592e2685aee68d05015f",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.7.13",
