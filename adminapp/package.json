  "private": true,
  "dependencies": {
    "@date-io/dayjs": "^2.16.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.116",
    "@mui/material": "^5.11.5",
    "@mui/styles": "^5.11.2",
    "@mui/utils": "^5.11.2",
    "@mui/x-data-grid": "^5.17.20",
    "@mui/x-date-pickers": "^5.0.15",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.2.3",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "humps": "^2.0.1",
    "lodash": "^4.17.21",
    "notistack": "^2.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-phone-number-input": "^3.2.16",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.7.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "analyze": "source-map-explorer ../build-adminapp/static/js/**/*.js",
    "analyze-win": "source-map-explorer ../build-adminapp/static/js/**/*.js --html source-explorer.html && explorer.exe source-explorer.html",
    "start": "CHOKIDAR_USEPOLLING=true PORT=22014 FAST_REFRESH=false react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "prettier-check": "prettier --no-error-on-unmatched-pattern --check '*.js' './tools/*.js' './src/**/*.js'",
    "prettier-fix": "prettier --no-error-on-unmatched-pattern --write '*.js' './tools/*.js' './src/**/*.js'",
    "eslint-check": "eslint --no-error-on-unmatched-pattern '*.js' './tools/*.js' './src/**/*.js'",
    "eslint-fix": "eslint --no-error-on-unmatched-pattern --fix '*.js' './tools/*.js' './src/**/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "prettier": "^2.8.3"
  }
}
